#LINEAR REGRESSION

â†’Run

library(ggplot2)
library (caret)
#load inbuilt data
data("mtcars")|
set.seed (89)
cor(mtcars) # co-relation matrix for the data
sample_index <- sample (1:nrow (mtcars), 0.8*nrow (mtcars))
train_data <- mtcars [sample_index, ]
test_data <- mtcars [-sample_index, ]
linearModel <- 1m(mpghp + drat + am, data = train_data)
summary (linearModel)
